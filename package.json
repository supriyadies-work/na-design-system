{
  "name": "@supriyadies-work/na-design-system",
  "version": "1.1.19",
  "description": "Design System package for Nisa Aulia Portfolio",
  "repository": {
    "type": "git",
    "url": "https://github.com/supriyadies-work/na-design-system.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/js/index.js",
  "module": "./dist/js/index.js",
  "types": "./dist/js/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/js/index.js",
      "require": "./dist/js/index.js",
      "types": "./dist/js/index.d.ts"
    },
    "./components": "./dist/js/components/index.js",
    "./components/*": "./dist/js/components/*/index.js",
    "./tokens": "./dist/js/tokens.js",
    "./css": "./dist/css/tokens.css",
    "./js/tokens": "./dist/js/tokens.js",
    "./utils": "./dist/js/utils/index.js",
    "./assets/*": "./dist/js/assets/*/index.js",
    "./public/*": "./public/*"
  },
  "files": [
    "dist",
    "public",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:tokens && npm run build:compile && npm run build:resolve-aliases && npm run build:copy-assets",
    "build:tokens": "style-dictionary build --config src/config/style-dictionary.config.js",
    "build:compile": "tsc || true",
    "build:resolve-aliases": "tsc-alias",
    "build:copy-assets": "node scripts/copy-assets.js",
    "build:watch": "npm run build:tokens -- --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "lottie-react": "^2.4.0",
    "next": "^14.0.0",
    "next-themes": "^0.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-quill": "^2.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "lottie-react": "^2.4.1",
    "next-themes": "^0.4.6",
    "react-quill": "^2.0.0",
    "style-dictionary": "^5.1.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "design-system",
    "react",
    "components",
    "tokens"
  ],
  "license": "MIT"
}