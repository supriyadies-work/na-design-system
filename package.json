{
  "name": "@supriyadies-work/na-design-system",
  "version": "1.0.0",
  "description": "Design System package for Nisa Aulia Portfolio",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./components": "./src/components/index.ts",
    "./components/*": "./src/components/*/index.ts",
    "./tokens": "./dist/js/tokens.js",
    "./css": "./dist/css/tokens.css",
    "./js/tokens": "./dist/js/tokens.js",
    "./utils": "./src/utils/index.ts"
  },
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:tokens",
    "build:tokens": "style-dictionary build --config src/config/style-dictionary.config.js",
    "build:watch": "npm run build:tokens -- --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "next": "^14.0.0",
    "next-themes": "^0.4.0",
    "lottie-react": "^2.4.0",
    "react-quill": "^2.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "style-dictionary": "^5.1.1",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "design-system",
    "react",
    "components",
    "tokens"
  ],
  "license": "MIT"
}

